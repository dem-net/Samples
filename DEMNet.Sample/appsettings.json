        "Acknowledgement": null
      },
      "UrlModel": {
        "UrlFormat": "https://maps.heigit.org/openmapsurfer/tiles/roads/webmercator/{z}/{x}/{y}.png"
      },
      "MaxZoom": 19,
      "MaxDegreeOfParallelism": 2
    },
    {
      "Name": "MapBox-Satellite",
      "Attribution": {
        "Text": "© MapxBox - OpenStreetMap contributors",
        "Url": "https://www.mapbox.com"
      },
      "UrlModel": {
        "UrlFormat": "https://api.mapbox.com/v4/mapbox.satellite/{z}/{x}/{y}.png?access_token={t}"
      },
      "MaxZoom": 23,
      "TokenUserSecretsKey": "MapBoxToken"
    }
  ]
}